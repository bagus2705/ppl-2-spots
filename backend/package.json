{
  "name": "backend-spots",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --hot index.js",
    "start": "bun index.js",
    "prisma:deploy": "prisma migrate deploy",
    "db:rm": "docker compose rm db -s -f -v",
    "db:up": "docker compose up db -d",
    "db:restart": "bun db:rm && bun db:up && sleep 1 && bun prisma:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "bun db:test:rm && bun db:test:up && sleep 1 && bun prisma:test:deploy"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "bun-types": "^1.0.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "js-base64": "^3.7.5",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.3.1",
    "multer": "^1.4.5-lts.1",
    "pino-http": "^8.5.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "pactum": "^3.5.1",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.4.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": [
    "@prisma/client"
  ]
}